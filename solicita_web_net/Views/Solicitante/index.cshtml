        <th>
            @Html.DisplayNameFor(model => model.sol_email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sol_foto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sol_telefone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sol_celular)
