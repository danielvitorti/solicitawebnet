        </th>
        <th>
            @Html.DisplayNameFor(model => model.sol_data_cadastro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sol_nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sol_email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sol_foto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sol_telefone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sol_celular)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sol_funcao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sol_data_cadastro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.sol_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.sol_id }) |
